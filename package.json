{
  "name": "@vercel/slack-bolt",
  "version": "0.1.5",
  "description": "A Vercel receiver for building Slack apps with Bolt and deploying them to Vercel",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel-labs/slack-bolt.git"
  },
  "bugs": {
    "url": "https://github.com/vercel-labs/slack-bolt/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "version-packages": "changeset version",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "prepare": "husky"
  },
  "keywords": [
    "vercel",
    "bolt",
    "slack"
  ],
  "author": "Vercel",
  "license": "MIT",
  "dependencies": {
    "@slack/logger": "^4.0.0",
    "@vercel/functions": "^2.2.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@changesets/cli": "^2.29.6",
    "@types/node": "^24.3.0",
    "husky": "^9.1.7",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@slack/bolt": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "@slack/bolt": {
      "optional": false
    }
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "publishConfig": {
    "access": "public"
  }
}
